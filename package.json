{
  "name": "@kronos-integration/service-consul",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/service-consul.mjs"
  },
  "description": "kronos service to integrate with consul",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "@kronos-integration/endpoint": "^9.4.34",
    "@kronos-integration/service": "^10.4.30",
    "consul": "^0.40.0",
    "model-attributes": "^4.1.18",
    "promise-repeat": "^0.5.0"
  },
  "devDependencies": {
    "@kronos-integration/service-http": "^10.6.128",
    "ava": "^4.3.1",
    "c8": "^7.12.0",
    "documentation": "^13.2.5",
    "semantic-release": "^19.0.3"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-consul.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-consul/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-consul#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
